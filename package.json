{
  "workspaces": [
    "src/frontend",
    "src/backend"
  ],
  "name": "dsc-portal",
  "version": "1.0.0",
  "description": "This project is being developed in a phased manner to ensure incremental improvements and feature additions. Below are the details of each stage:",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "npm --prefix src/frontend start",
    "start:backend": "npm --prefix src/backend start",
    "dev:backend": "npm --prefix src/backend run dev",
    "dev:frontend": "npm --prefix src/frontend run dev",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "framer-motion": "^5.3.0",
    "lucide-react": "^0.469.0",
    "motion": "^11.14.1",
    "react-icons": "^5.4.0"
  }
}
